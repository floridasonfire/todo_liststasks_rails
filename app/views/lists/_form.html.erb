<%= render "layouts/errors", :object => @list %>

<div class="well col-sm-6">
  <%= form_for @list do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="form-group">
      <%= f.label :description %>
      <%= f.text_field :description %>
    </div>
    <%= f.submit :class => 'btn btn-warning'%>
  <% end %>
  <br />
  <p><%= link_to "Return to lists", lists_path, :class => 'btn btn-info'%></p>
</div>
